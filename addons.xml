<?xml version="1.0" encoding="UTF-8"?>
<addons>
<addon id="plugin.video.cloudstream" version="0.1.0" name="CloudStream" provider-name="ddurdle">
    <requires>
        <import addon="xbmc.python" version="2.1.0"/>
    </requires>
    <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>video</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>A cloud player for various cloud-streaming services</summary>
        <description>Stream music and video content from various cloud-streaming services
                                supports: daclips.in, gorillavid.in, movpod.in, thefile.me, bestreams.net, uptobox.com,
                                sharerep.com, thevideo.me, filenuke.com, sharesix.com, mightyupload.com, vidhog.com,
                                vidspot.net, www.uploadc.com, hcbit.com</description>
        <platform>all</platform>
    </extension>
</addon>
<addon id="plugin.video.layanmovie" version="0.0.3" name="layanmovie" provider-name="ddurdle">
    <requires>
        <import addon="xbmc.python" version="2.1.0"/>
        <import addon="plugin.video.gdrive" version="0.2.7"/>
    </requires>
    <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>video</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>Site player for layanmovie.com</summary>
        <description>Site player for layanmovie.com</description>
        <platform>all</platform>
    </extension>
</addon>
<addon id="plugin.video.gdrive" version="0.4.7" name="gdrive" provider-name="ddurdle">
    <requires>
        <import addon="xbmc.python" version="2.1.0"/>
    </requires>
    <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>video audio</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>Plays music and video from your Google Drive account</summary>
        <description>Plays music and video from your Google Drive account</description>
        <platform>all</platform>
    </extension>
</addon>
</addons>
